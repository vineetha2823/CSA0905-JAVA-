import java.util.*;
class Palindrome
 {
    public static void main(String arg[])
{
        int n,sum=0,rem,temp;
	Scanner s=new Scanner(System.in);
	System.out.println("Enter number: ");
	n=s.nextInt();
	temp=n;
	while(n!=0)
	{
	rem=n%10;
	sum=sum*10+rem;
	n=n/10;
	}
	if(temp==sum)
	{
	System.out.println("Palindrome number");
	}
	else
	{
	System.out.println("Not a Palindrome number");
	}
    }
}
