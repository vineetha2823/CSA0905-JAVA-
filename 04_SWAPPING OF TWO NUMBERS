import java.util.*;
class Swap
{
	public static void main(String arg[])
{
	int a,b,temp;
	Scanner s=new Scanner(System.in);
	System.out.println("Enter a value: ");
	a=s.nextInt();
	System.out.println("Enter b value: ");
	b=s.nextInt();
	temp=a;
	a=b;
	b=temp;
	System.out.println("a: "+a);
	System.out.println("b: "+b);
}
}
