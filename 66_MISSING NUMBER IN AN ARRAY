import java.util.*;
class Missing
{
	public static void main(String args[])
	{
		int sum=0,i,n;
		Scanner s=new Scanner(System.in);
		System.out.println("enter n: ");
		n=s.nextInt();
		 int[] a = new int[n];
		int total=(n*(n+1))/2;
        	for(i=0;i<n;i++) 
		{
            		System.out.print("Enter number " + (i + 1) + ": ");
            		a[i] = s.nextInt();
       		 }
		for(i=0;i<n;i++)
		{
			sum=sum+a[i];
		}
		System.out.println("missing number is:"+(total-sum));
}
}
