import java.util.*;
class Squarecube
{
public static void main(String args[])
{
int square,cube,n;
Scanner s=new Scanner(System.in);
System.out.println("enter the number:");
n=s.nextInt();
square=n*n;
System.out.println("square of number:"+square);
cube=n*n*n;
System.out.println("cube of number:"+cube);
}
}
