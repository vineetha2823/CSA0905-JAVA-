import java.util.*;
class Floyd 
{
	public static void main(String[] args)
	{
		int n;
		int i, j, k = 1;
                Scanner s=new Scanner(System.in);
		n=s.nextInt();
		for (i = 1; i <= n; i++) 
		{
			for (j = 1; j <= i; j++) 
			{
				System.out.print(k + " ");
				k++;
			}
			System.out.println();
		}
	}
}
