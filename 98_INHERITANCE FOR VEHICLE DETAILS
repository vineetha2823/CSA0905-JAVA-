import java.util.Scanner;
class Vehicle
{
void detail(int n6,int lu)
{
int l;
int n;
l=lu;
n=n6;
}
}
class car extends Vehicle
{
void display(int l4,int n7)
{

int n9,l9;
l9=l4;
n9=n7;
System.out.println("car deatils");
System.out.println("enter the owner:"+l9);
System.out.println("enter the number:"+n9);
}
}
class truck extends car
{
void show(int lw,int n8)
{
int n2,lr;
n2=n8;
lr=lw;
System.out.println("truck deatils");
System.out.println("enter the owner:"+lr);
System.out.println("enter the number:"+n2);
}
}
class Motorcycle extends truck
{
void cal(int n9)
{
int n3;
n3=n9;
System.out.println("motorcycle deatils");
System.out.println("enter the number:"+n3);
}
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
System.out.println("enter the number:");
int n=s.nextInt();
System.out.println("enter the license:");
int l1=s.nextInt();
System.out.println("enter the number:");
int nt=s.nextInt();
System.out.println("enter the license:");
int l0=s.nextInt();
System.out.println("enter the number:");
int n0=s.nextInt();
Motorcycle obj=new Motorcycle();
obj.cal(n);
obj.display(l1,nt);
obj.show(l0,n0);
}
}
